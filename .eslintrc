{
  "extends": "airbnb",
  "env": {
    "es6": false,
    "node": true,
    "jquery": true
  },
  "globals": {
    "electron": true,
    "translateInterface": true,
    "editor": true,
    "conf": true,
    "i18n": true,
    "dialog": true,
    "phpPath": true,
    "runner": true,
    "isMainWindow": true,
    "binaryAdd": true,
    "binaryDialogAdd": true,
    "binaryUpdateList": true,
    "binarySetNewDefault": true,
    "updatePhpPath": true,
    "ace": true,
    "ipc": true,
    "goFullScreen": true,
    "runCode": true,
    "setOutput": true,
    "checkPresentation": true,
    "presentationSingle": true,
    "presentationMulti": true,
    "mode": true,
    "remote": true,
    "shell": true,
    "settingsDefault": true,
    "Path": true,
    "packageInfo": true,
    "fs": true,
    "chdir": true,
    "Split": true,
    "editorUnbind": true,
    "setDefaultSettings": true,
    "renderApp": true,
    "binaryGetVersion": true,
    "dummyName": true,
    "tmpDir": true,
    "checkForUpdates": true,
    "setSearchingUpdate": true,
    "themesList": true
  },
  "rules": {
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "func-names": 0,
    "prefer-arrow-callback:": 0,
    "prefer-template": 0,
    "comma-dangle": 0,
    "no-unused-vars": 0,
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
