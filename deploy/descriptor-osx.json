{
    "package": {
        "name": "php-assistant",
        "repo": "mac-osx",
        "subject": "rafajaques",
        "desc": "A cross-platform desktop PHP runner",
        "website_url": "www.github.com/rafajaques",
        "issue_tracker_url": "https://github.com/rafajaques/php-assistant/issues",
        "vcs_url": "https://github.com/rafajaques/php-assistant.git",
        "github_use_tag_release_notes": true,
        "github_release_notes_file": "RELEASE.txt",
        "licenses": ["MIT"],
        "labels": ["php", "assistant", "sandbox"],
        "public_download_numbers": false,
        "public_stats": false
    },

    "version": {
        "name": "0.0.7",
        "desc": "Minor release",
        "released": "2016-05-29",
        "vcs_tag": "0.0.7",
        "gpgSign": false
    },

    "files":
    [
      {
        "includePattern": "dist/osx/(.*\.dmg)",
        "uploadPattern": "/$1",
        "matrixParams": {
          "override": 1
        }
      }
    ],
    "publish": true
}
